  body
    .main
      section.signup
        .container
          .signup-content
            .signup-form
              h2.form-title Sign up
              form.form.register-form#register-form(method='POST')
                .form-group
                  label(for='name')
                    i.zmdi.zmdi-account.material-icons-name
                  input#name(type='text' name='name' placeholder='Firstname Lastname')
                .form-group
                  label(for='email')
                    i.zmdi.zmdi-email
                  input#email(type='email' name='email' placeholder='Your Email')
                .form-group
                  label(for='number')
                    i.zmdi.zmdi-phone
                  input#phone(type='text' name='phone' placeholder='Your Phone number')
                .form-group
                  label(for='pass')
                    i.zmdi.zmdi-lock
                  input#password(type='password' name='pass' placeholder='Password')
                .form-group
                  label(for='re-pass')
                    i.zmdi.zmdi-lock-outline
                  input#confirm_password(type='password' name='re_pass' placeholder='Repeat your password')
                .form-group
                  i Male
                  input.gender#male(type='radio' name='gender' value='male')
                  i Female
                  input.gender#female(type='radio' name='gender' value='male')                 
                .form-group
                  i Address
                  br
                  textarea.address(name="address") 
                  
                //- .form-group
                //-   input#agree-term.agree-term(type='checkbox' name='agree-term')
                //-   label.label-agree-term(for='agree-term')
                //-     span
                //-       span
                //-     | I agree all statements in 
                //-     a.term-service(href='#') Terms of service
                .form-group.form-button
                  input#signup.form-submit(type='submit' name='signup' value='Register')
            .signup-image
              figure
                img(src='images/signup-image.jpg' alt='sing up image')
              a.signup-image-link(href='login') I am already a member
