  body
    .main
      section.sign-in
        .container
          .signin-content
            .signin-image
              figure
                img(src='images/signin-image.jpg' alt='sing up image')
              a.signup-image-link(href='register') Create an account
            .signin-form
              h2.form-title Log in
              form.form.register-form#login-form(method='POST')
                .form-group
                  label(for='your_name')
                    i.zmdi.zmdi-account.material-icons-name
                  input#email(type='text' name='email' placeholder='Your Email')
                .form-group
                  label(for='your_pass')
                    i.zmdi.zmdi-lock
                  input#password(type='password' name='your_pass' placeholder='Password')
                .form-group
                  input#remember-me.agree-term(type='checkbox' name='remember-me')
                  label.label-agree-term(for='remember-me')
                    span
                      span
                    | Remember me
                .form-group.form-button
                  input#signin.form-submit(type='submit' name='signin' value='Log in')
